{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-write-text-file",
    "fs:allow-read-text-file",
    "fs:allow-write-file",
    "fs:allow-read-file",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:scope-home-recursive"
  ]
}